  agent any {
    Stages{
      stage("build"){
        steps{
          echo 'building the applicaion'
          
        }
             stage("Test"){
        steps{
           echo 'testing the applicaion'
          
          
        }
          }
                    stage("Deploy"){
        steps{
           echo 'Deploying the application'
          
        }
          }
    }
  }
  
}

  
    
